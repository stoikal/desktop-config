# label=MEM

# [disk]
# command=$HOME/Projects/personal/desktop-config/bin/i3blocks/disk_avail.sh
# interval=60
# label=DISK

[wifi]
command=$HOME/Projects/personal/desktop-config/bin/i3blocks/wifi_ssid.sh
interval=10
label=WIFI

[battery]
command=$HOME/Projects/personal/desktop-config/bin/i3blocks/battery_status.sh
interval=30
label=BAT

[time]
command=$HOME/Projects/personal/desktop-config/bin/i3blocks/time.sh
interval=60
